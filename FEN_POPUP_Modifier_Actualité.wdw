        code : |1-
         
         // Le titre
         SI SAI_Titre ~= "" ALORS
         	Erreur("Veuillez entrer un titre.")
         	RepriseSaisie(SAI_Titre)
         FIN
         
         
         // La description
         si EDHTML_Description ~= "" ALORS
         	Erreur("Veuillez entrer une description commerciale.")
         	RepriseSaisie(EDHTML_Description)
         FIN
         
         
         sablier(vrai)
         Si gnIDActualité > 0 ALORS
         	// Modification
         	si ModifierActualité(gnIDActualité,EDHTML_Description,SAI_Titre) ALORS
         		ToastAffiche("Actualité modifiée avec succès")
         		FEN_Principale.RafraichitActualités()
         		ferme()
         	SINON
         		erreurws("Erreur lors de la modification de la nouveauté.")
         	FIN
         SINON
         	// Ajout
         	si AjouterActualité(EDHTML_Description,SAI_Titre)
         		ToastAffiche("Actualité ajoutée avec succès")
         		FEN_Principale.RafraichitActualités()
         		Ferme()
         	SINON
         		ErreurWS("Erreur lors de l'ajout de l'actualité.")
         	FIN
         FIN
         Sablier(faux)
