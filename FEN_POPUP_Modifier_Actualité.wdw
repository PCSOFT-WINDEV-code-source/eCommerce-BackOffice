      PROCEDURE FEN_POPUP_Modifier_Actualité(gnIDActualité est un entier)
      
      gstActualité	est une STCOL_Actualités
      
      // Recherche de la actualité
      SI gnIDActualité = 0 ALORS
      	// Nouvelle actualité
      	BTN_Modifier = "Ajouter"
      SINON
      	// Modification
      	POUR TOUT stActualité de gtabActualités
      		SI stActualité.IDActualités = gnIDActualité ALORS
      			gstActualité = stActualité
      			SORTIR
      		FIN
      	FIN
      	
      	SAI_Titre = gstActualité.Titre
      	EDHTML_Description = gstActualité.TexteActualité
      FIN
      
