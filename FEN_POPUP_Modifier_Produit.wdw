     name : RemplisComboFamille
     procedure_id : 2238296101500486102
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //RemplisComboFamille ( [<nParent> est entier [, <nNiveau> est entier]])
      //
      // Paramètres :
      //	nParent (entier - valeur par défaut=0) : < indiquez ici le rôle de nParent >
      //	nNiveau (entier - valeur par défaut=0) : <indiquez ici le rôle de nNiveau>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE RemplisComboFamille(nParent est un entier = 0, nNiveau est un entier = 0)
      
      // Variable
      sNiveau 			est un chaine
      nID 				est un entier
      
      SI nParent = 0 ALORS
      	ListeSupprimeTout(COMBO_Famille)
      	ListeAjoute(COMBO_Famille,"Toutes les familles" + gLien(-1))
      	ListeSelectPlus(COMBO_Famille,1)
      FIN
      
      POur tout stFamille de gtabFamilles
      	Si stFamille.IDFamilleParent = nParent ALORS
      		nID = stFamille.IDFamille
      	
      		// Niveau
      		sNiveau = ""
      		POUR j = 1 _a_ nNiveau
      			sNiveau += "---"
      		FIN
      		
      		ListeAjoute(COMBO_Famille,sNiveau + stFamille.Libellé + gLien(nID))
      		
      		RemplisComboFamille(nID,nNiveau + 1)
      	FIN
      FIN
      
      
