     p_codes :
      -
        code : |1-
         
         nIDProduit est un entier
         
         //=============================================================
         // Vérification des paramètres
         
         // La référence
         SI SAI_Référence ~= "" ALORS	
         	Erreur(<§$0000§>)
         	RepriseSaisie(SAI_Référence)		
         FIN
         
         // Le libellé
         SI SAI_Libellé ~= "" ALORS
         	Erreur(<§$0001§>)
         	RepriseSaisie(SAI_Libellé)
         FIN
         
         SI SAI_Prix ~= "" ALORS
         	Erreur(<§$0002§>)
         	RepriseSaisie(SAI_Libellé)	
         FIN
         
         // La famille
         SI COMBO_Famille <= 0 ALORS
         	Erreur(<§$0003§>)
         	RepriseSaisie()	
         FIN
         
         // Vérification du taux de taxe
         SI COMBO_Taxe <=0 ALORS
         	Erreur(<§$0004§>)
         	RepriseSaisie()
         FIN
         
         //=============================================================
         // Validation
         // Est ce que l'on est en ajout ou en modification
         SI gnIDProduit = 0 ALORS
         	// Ajout du produit
         	nIDProduitAjouté est un entier
         	nIDProduitAjouté =  AjouterProduit(COMBO_Famille..ValeurMémorisée,SAI_Référence,SAI_Libellé,EDHTML_Description,SAI_Prix,COMBO_Taxe..ValeurMémorisée,faux)
         	SI nIDProduitAjouté >0 ALORS
         		ToastAffiche("Le produit a bien été ajouté")
         		FEN_Principale.RechercheProduits(nIDProduitAjouté)
         		ferme()
         	SINON			
         		ErreurWS("Le produit n'a pas pu être ajouté")
         		RETOUR
         	FIN
         	
         SINON
         	nIDProduit = gstProduit.IDProduit
         	// Modification du produit
         	SI ModifierProduit(nIDProduit,COMBO_Famille..ValeurMémorisée,SAI_Référence,SAI_Libellé,EDHTML_Description,SAI_Prix,COMBO_Taxe..ValeurMémorisée,faux) = Vrai ALORS
         		ToastAffiche("Le produit a bien été modifié")
         		FEN_Principale.RechercheProduits(nIDProduit)
         		ferme()
         	SINON
         		ErreurWS("Le produit n'a pas pu être modifié")
         		RETOUR
         	FIN
         FIN
