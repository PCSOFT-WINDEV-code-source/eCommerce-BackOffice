         nIDProduit est un entier
         
         nIDProduit = ouvre(FEN_Sélection_Produit)
         
         SI nIDProduit = 0 ALORS
         	BTN_Produit = "Aucun produit sélectionné"
         	gnIDProduitSélectionné = -1
         SINON
         	gnIDProduitSélectionné = nIDProduit
         	stProduit = ChargeProduit(gnIDProduitSélectionné)
         	BTN_Produit = stProduit.LibelléProduit
