  -
    name : BTN_Modifier
    identifier : 0x2f185804db0851a2
    internal_properties : 
    properties :
     x : 235
     y : 445
     width : 156
     height : 30
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // La description
         si EDHTML_Description ~= "" ALORS
         	Erreur("Veuillez entrer une description commerciale.")
         	RepriseSaisie(EDHTML_Description)
         FIN
         
         // Le produit
         SI gnIDProduitSélectionné < 1 ALORS	
         	Erreur("Vous devez sélectionner un produit.")
         	RETOUR
         FIN
         
         
         sablier(vrai)
         Si gnIDNouveauté > 0 ALORS
         	// Modification
         	dDate est une date = gstNouveauté.Date
         	si ModifieNouveauté(gnIDNouveauté,gnIDProduitSélectionné,EDHTML_Description,dDate) ALORS
         		ToastAffiche("Nouveauté modifiée avec succès")
         		FEN_Principale.RafraichitNouveautés()
         		ferme()
         	SINON
         		erreurws("Erreur lors de la modification de la nouveauté.")
         	FIN
         SINON
         	// Ajout
         	Si AjouteNouveauté(gnIDProduitSélectionné,EDHTML_Description,DateDuJour())
         		ToastAffiche("Nouveauté ajoutée avec succès")
         		FEN_Principale.RafraichitNouveautés()
         		Ferme()
         	SINON
         		ErreurWS("Erreur lors de l'ajout de la nouveauté.")
         	FIN
         FIN
         Sablier(faux)
