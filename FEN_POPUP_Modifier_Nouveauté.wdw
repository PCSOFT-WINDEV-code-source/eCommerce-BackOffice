   -
     code : |1-
      
      procedure FEN_POPUP_Modifier_Nouveauté(gnIDNouveauté est un entier)
      
      gstNouveauté 				est une STCOL_Nouveauté
      gnIDProduitSélectionné 		est un entier
      stProduit est un STCOL_Produit
      
      // Recherche de la nouveauté
      Si gnIDNouveauté = 0 ALORS
      	// Nouvelle nouveauté
      	BTN_Modifier = "Ajouter"
      SINON
      	// Modification
      	pour tout stNouveauté de gtabNouveautés
      		si stNouveauté.IDNouveauté = gnIDNouveauté ALORS
      			gstNouveauté = stNouveauté
      			sortir
      		FIN
      	FIN
      	
      	EDHTML_Description = gstNouveauté.Description
      	
      	// Produit sélectionné
      	gnIDProduitSélectionné = gstNouveauté.IDProduit
      	stProduit = ChargeProduit(gnIDProduitSélectionné)
      	BTN_Produit = stProduit.LibelléProduit
      FIN
