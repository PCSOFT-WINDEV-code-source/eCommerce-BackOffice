     code : |1+
      
      procedure FEN_POPUP_Modifier_Promotion(gnIDPromotion est un entier = 0)
      
      // Variables globales
      gstPromotion 			est une STCOL_Promotion
      gnIDProduitSélectionné 	est un entier
      gmoPrixOriginal 		est un monétaire
      
      SI gnIDPromotion = 0 ALORS
      	// Nouvelle promotion
      	BTN_Modifier = "Ajouter"
      	
      	// Dates
      	SAI_DateDébut_Promotion = DateDuJour()
      	dDateJour est une Date
      	dDateJour+=7
      	SAI_DateFin_Promotion = dDateJour
      	
      	// Interrupteurs
      	INT_Promo_Principale[1] = faux
      	INT_QuantitéLimité[1] = faux
      	
      	// Groupe
      	GR_Bandeaux..Etat = grisé
      SINON
      	// Recherche de la promo
      	POUR TOUT stPromotion de gtabPromotions
      		SI stPromotion.IDPromotion = gnIDPromotion ALORS
      			gstPromotion = stPromotion
      			
      			// Chargement de la promotion
      			ChargePromotion()
      			
      			sortir
      		FIN
      	FIN
      FIN
