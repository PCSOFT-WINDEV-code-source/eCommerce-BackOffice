  procedures :
   -
     name : ChargePromotion
     procedure_id : 2240575831490553719
     type_code : 14
     code : |1-
      
      // Résumé : Charge la promotion dans la fenêtre
      //
      PROCEDURE ChargePromotion()
      
      // Variables
      stProduit est un STCOL_Produit
      
      // Produit sélectionné
      gnIDProduitSélectionné = gstPromotion.IDProduit
      stProduit = ChargeProduit(gnIDProduitSélectionné)
      gmoPrixOriginal = stProduit.PrixUnitaireHT
      BTN_Produit = stProduit.LibelléProduit
      LIB_PrixOriginal = gmoPrixOriginal
      
      // Dates
      SAI_DateDébut_Promotion = gstPromotion.DateDébut
      SAI_DateFin_Promotion = gstPromotion.Datefin
      
      // Description
      SAI_Titre = gstPromotion.TitrePromotion
      SAI_TexteAlternatif = gstPromotion.TexteRemplacement
      EDHTML_Description = gstPromotion.Description
      
      // Prix
      SAI_PrixPromo = gstPromotion.PrixUnitaireHT
      RafraichîtPourcentagePromo()
      
      // interrupteurs
      INT_Promo_Principale[1] = gstPromotion.EstPrincipale
      INT_QuantitéLimité[1] = gstPromotion.QuantitéLimitée
      
      Si gstPromotion.QuantitéLimitée = vrai ALORS
      	SAI_Quantité = gstPromotion.Quantité
      	SAI_Quantité..etat = actif
      FIN
      
      // Bandeaux de promotion
      AfficherBandeauxPromotion()
     type : 458752
   -
     name : AfficherBandeauxPromotion
     procedure_id : 2240585632606043000
     type_code : 14
     code : |1+
      // Résumé : Affiche les bandeaux de promotion
      // Syntaxe :
      // AfficherBandeauxPromotion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficherBandeauxPromotion()
      
      // Variables
      sImageDistanteCarré 	est une chaine
      sImageDistanteRect 		est une chaine
      bufImageCarré 			est un buffer
      bufImageRect 		est un buffer
      
      // Si on a un identifiant de promotion, on affiche la promotion en cours en base
      SI gnIDPromotion > 0 ALORS
      	sablier(vrai)
      	
      	// Image carré
      	sImageDistanteCarré =  LirePhotoPromotion(gnIDPromotion,1)
      	Si sImageDistanteCarré <> "" ALORS
      		sImageDistanteCarré =  gsURLSite + ["/"] + sImageDistanteCarré
      		Si HTTPRequête(sImageDistanteCarré) ALORS
      			bufImageCarré = HTTPDonneRésultat(httpRésultat)
      		FIN
      	FIN
      	
      	// Image rectangulaire
      	sImageDistanteRect =  LirePhotoPromotion(gnIDPromotion,3)
      	SI sImageDistanteRect <> "" ALORS
      		sImageDistanteRect =  gsURLSite + ["/"] + sImageDistanteRect
      		SI HTTPRequête(sImageDistanteRect) ALORS
      			bufImageRect = HTTPDonneRésultat(httpRésultat)
      		FIN
      	FIN
      	
      	sablier(faux)
      	
      	IMG_Bandeau_Carré = bufImageCarré
      	IMG_Bandeau_Rectangulaire = bufImageRect
      FIN
     type : 458752
   -
     name : ModifieBandeau
     procedure_id : 2240586285441092818
     type_code : 14
     code : |1-
      // Résumé : Modifie le bandeau carré
      // Syntaxe :
      //ModifieBandeau (<sFichier> est chaîne ANSI, <nRole> est entier)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : Le chemin de l'image
      //	nRole (entier) : Le rôle de l'image
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ModifieBandeau(sFichier est une chaine,nRole est un entier)
      
      bufFichier est un buffer
      
      // Chargement du fichier
      bufFichier = fChargeBuffer(sFichier)
      
      // Ajout en base	
      AssocierPhotoPromotion(gnIDPromotion,nRole,bufFichier)
      
      AfficherBandeauxPromotion()
     type : 458752
   -
     name : ModifiePromotion
     procedure_id : 2240591310568350843
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ModifiePromotion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      //	nIDProduit : <indiquez ici le rôle de nIDProduit>
      //	COMBO_Famille : <indiquez ici le rôle de COMBO_Famille>
      //	SAI_Référence : <indiquez ici le rôle de SAI_Référence>
      //	SAI_Libellé : <indiquez ici le rôle de SAI_Libellé>
      //	SAI_Prix : <indiquez ici le rôle de SAI_Prix>
      //	COMBO_Taxe : <indiquez ici le rôle de COMBO_Taxe>
      PROCEDURE ModifiePromotion()
      
      //=============================================================
      // Vérification des paramètres
      
      // Le produit
      SI gnIDProduitSélectionné < 1 ALORS	
      	Erreur("Vous devez sélectionner un produit.")
      	retour
      FIN
      
      // Le titre
      SI SAI_Titre ~= "" ALORS
      	Erreur("Vous devez saisir un titre.")
      	RepriseSaisie(SAI_Titre)
      FIN
      
      // Le texte alternatif
      SI SAI_TexteAlternatif ~= "" ALORS
      	Erreur("Vous devez saisir un texte alternatif.")
      	RepriseSaisie(SAI_TexteAlternatif)
      FIN
      
      // Le prix
      SI SAI_PrixPromo  <= 0  ALORS
      	Erreur("Vous devez saisir un prix..")
      	RepriseSaisie(SAI_PrixPromo)
      FIN
      
      // Remplissage de la structure
      gstPromotion.DateDébut = SAI_DateDébut_Promotion
      gstPromotion.DateFin = SAI_DateFin_Promotion
      gstPromotion.Description = EDHTML_Description
      gstPromotion.EstPrincipale = INT_Promo_Principale[1]
      gstPromotion.IDProduit = gnIDProduitSélectionné
      gstPromotion.PrixUnitaireHT = SAI_PrixPromo
      gstPromotion.Quantité = SAI_Quantité
      gstPromotion.QuantitéLimitée = INT_QuantitéLimité[1]
      gstPromotion.TexteRemplacement = SAI_TexteAlternatif
      gstPromotion.TitrePromotion = SAI_Titre
      
      //=============================================================
      // Validation
      // Est ce que l'on est en ajout ou en modification
      SI gnIDPromotion = 0 ALORS
      	// Ajout de la promo
      	SI AjouterPromotion(gstPromotion) ALORS
      		ToastAffiche("La promotion a bien été ajoutée")
      		FEN_Principale.RecherchePromotions()
      		Ferme()
      	SINON			
      		ErreurWS("La promotion n'a pas pu être ajoutée.")
      		RETOUR
      	FIN
      	
      SINON
      	// Ajout de la promo
      	pParam est un ModifierPromotion
      	pParam.nIDPromotionRecherchée = gnIDPromotion
      	pParam.stContenuPromotion = gstPromotion
      	SI ModifierPromotion(pParam) ALORS
      		ToastAffiche("La promotion a bien été modifiée")
      		FEN_Principale.RecherchePromotions()
      		Ferme()
      	SINON			
      		ErreurWS("La promotion n'a pas pu être modifiée")
      		RETOUR
      	FIN
      FIN
     type : 458752
   -
     name : RafraichîtPourcentagePromo
     procedure_id : 2247656995639007102
     type_code : 14
     code : |1-
      // Résumé : Rafraîchit le taux de promotion
      // Syntaxe :
      //RafraichîtPourcentagePromo ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RafraichîtPourcentagePromo()
      
      LIB_PrixOriginal = gmoPrixOriginal
      si SAI_Taux_Promotion > 0 ALORS
      	SAI_PrixPromo = gmoPrixOriginal - (gmoPrixOriginal * SAI_Taux_Promotion / 100)
      SINON
      	Si SAI_PrixPromo > 0 ALORS
      		SAI_Taux_Promotion = (gmoPrixOriginal - SAI_PrixPromo) * 100 / gmoPrixOriginal
      	SINON
      		SAI_PrixPromo = gmoPrixOriginal
      	FIN
      FIN
     type : 458752
